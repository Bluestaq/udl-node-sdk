{
  "dxt_version": "0.2",
  "name": "unified-data-library-mcp",
  "version": "0.1.0-alpha.17",
  "description": "The official MCP Server for the Unifieddatalibrary API",
  "author": {
    "name": "Unifieddatalibrary",
    "email": "unifieddatalibrary@bluestaq.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Bluestaq/udl-node-sdk.git"
  },
  "homepage": "https://github.com/Bluestaq/udl-node-sdk/tree/main/packages/mcp-server#readme",
  "documentation": "https://unifieddatalibrary.com/storefront/#/api-docs",
  "server": {
    "type": "node",
    "entry_point": "index.js",
    "mcp_config": {
      "command": "node",
      "args": ["${__dirname}/index.js"],
      "env": {
        "UDL_ACCESS_TOKEN": "${user_config.UDL_ACCESS_TOKEN}",
        "UDL_AUTH_PASSWORD": "${user_config.UDL_AUTH_PASSWORD}",
        "UDL_AUTH_USERNAME": "${user_config.UDL_AUTH_USERNAME}"
      }
    }
  },
  "user_config": {
    "UDL_ACCESS_TOKEN": {
      "title": "access_token",
      "description": "Access token for Bearer Authentication",
      "required": false,
      "type": "string"
    },
    "UDL_AUTH_PASSWORD": {
      "title": "password",
      "description": "Password for HTTP Basic Authentication",
      "required": false,
      "type": "string"
    },
    "UDL_AUTH_USERNAME": {
      "title": "username",
      "description": "Username for HTTP Basic Authentication",
      "required": false,
      "type": "string"
    }
  },
  "tools": [],
  "tools_generated": true,
  "compatibility": {
    "runtimes": {
      "node": ">=18.0.0"
    }
  },
  "keywords": ["api"]
}
